{
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "class-methods-use-this": "off",
    "max-len": "off",
    "indent": 0
  },
  "plugins": ["webdriverio"],
  "env": {
    "webdriverio/wdio": true,
    "mocha": true,
    "browser": true
  },
  "globals": {
    "page": true
  }
}